{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
    "DingDingRobotSetting": {
        "Secret": "xxxxxxxx", //加签Secret【安全设置选择加签】
        "SendUrl": "https://oapi.dingtalk.com/robot/send?access_token=xxxxxxx", //钉钉机器人消息发送地址
        "PingTimes": 10, //每个IP ping的次数  默认：10次
        "PingWarningTime": 1000, //单位毫秒 当ping往返时间大于该值时，则钉钉机器人发送通知 默认：1秒
        "PollingTime": 10000 //后台服务轮询检查--单位毫秒 默认：10秒
    },
  "AllowedHosts": "*"
}
